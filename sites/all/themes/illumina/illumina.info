;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = bootstrap/css/bootstrap.css
stylesheets[all][] = css/override.css
stylesheets[all][] = css/style.css
stylesheets[all][] = css/type.css
stylesheets[all][] = css/color.css
stylesheets[all][] = css/link.css
stylesheets[all][] = css/block.css
stylesheets[all][] = css/menu.css
stylesheets[all][] = css/responsive.css

;;;;;;;;;;;;;;;;;;;;;
;; Javascript
;;;;;;;;;;;;;;;;;;;;;
scripts[] = bootstrap/js/bootstrap.js
scripts[] = js/script.js
scripts[] = js/jquery-migrate-1.2.1.min.js